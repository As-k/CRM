    <TableRow
        android:layout_margin="2dp" >
        <TextView
            style="@style/text_view_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:text="Contact No. :"
            />
        <TextView
            android:id="@+id/call_log_cno"
            style="@style/text_view_style"
